{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/routes/(.*)",
      "dest": "/routes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ]
}
