{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/index.ts"
    },
    {
      "src": "/routes/(.*)",
      "dest": "/src/routes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/api/index.ts"
    }
  ]
}
