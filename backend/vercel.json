{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/routes/(.*)",
      "dest": "/routes/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.ts"
    }
  ]
}